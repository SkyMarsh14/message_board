<section>
  <ul>
    <div>There are <%=messages.length %> messages in inbox</div>
    <% for(let i=0;i<messages.length;i++){ %>
        <li>
            <div class="user"><%=messages[i].user %></div>
            <div class="text"><%=messages[i].text %></div>
            <div class="inputDate"><%=messages[i].added %></div>
            <button type="button"><a href="/details/<%=i %>" >Open</a></button>
        </li>
    <% } %> 
  </ul>
</section>
